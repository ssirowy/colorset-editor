{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "jsx-boolean-value": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": false,
    "no-namespace": [true, "allow-declarations"]
  },
  "rulesDirectory": []
}
